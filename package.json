{
  "name": "flutter-canvas-ts",
  "version": "1.0.0",
  "description": "A TypeScript implementation of Flutter painting library, providing Flutter-like drawing APIs for web environments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fi2zz/flutter-canvas-ts.git"
  },
  "keywords": [
    "flutter",
    "canvas",
    "typescript",
    "painting",
    "drawing",
    "graphics",
    "web",
    "html5"
  ],
  "author": "Fi2zz",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Fi2zz/flutter-canvas-ts/issues"
  },
  "homepage": "https://github.com/Fi2zz/flutter-canvas-ts#readme",
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "@vitest/ui": "^4.0.4",
    "happy-dom": "^20.0.8",
    "jsdom": "^27.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  }
}
